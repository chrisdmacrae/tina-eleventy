    "target": "ESNext",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "skipLibCheck": true
  },
  "include": [
    "src/cms/*.ts",
    "src/cms/*.tsx"
  ],
  "exclude": ["node_modules"]
}